#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sweety/Downloads/artisans_backend/node_modules/.pnpm/chalk-animation@2.0.2/node_modules/chalk-animation/node_modules:/Users/sweety/Downloads/artisans_backend/node_modules/.pnpm/chalk-animation@2.0.2/node_modules:/Users/sweety/Downloads/artisans_backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sweety/Downloads/artisans_backend/node_modules/.pnpm/chalk-animation@2.0.2/node_modules/chalk-animation/node_modules:/Users/sweety/Downloads/artisans_backend/node_modules/.pnpm/chalk-animation@2.0.2/node_modules:/Users/sweety/Downloads/artisans_backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../chalk-animation/cli.js" "$@"
else
  exec node  "$basedir/../chalk-animation/cli.js" "$@"
fi
